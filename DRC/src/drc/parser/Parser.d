  Инструкция разборИнструкцииПереход()
  {
    пропусти(T.Переход);
    Идентификатор* идент;
    Выражение вырРеле;
    switch (сема.вид)
    {
    case T.Реле:
      идент = сема.идент;
      далее();
      if (сема.вид == T.ТочкаЗапятая)
        break;
      вырРеле = разборВыражения();
      break;
