module ll.Comdat;
import ll.Types;

extern (C) {


    enum ЛЛВидВыбораКомдат
    {
      Лыбой,        ///< Компоновщик может выбрать любой COMDAT.
      ТочноеСовпадение, ///< Данные, на которые ссылается COMDAT,
                                         ///< должны быть одинаковыми.
      Наибольший,    ///< Компоновщик выберет наибольший
                                         ///< COMDAT.
      БезДубликатов, ///< Никакой иной модуль не может указывать этот
                                           ///< COMDAT.
      ОдинаковыйРазмер ///< Данные, на которые ссылается COMDAT,
                                      ///< должны быть одинакового размера.
    } 


    /**
     * Возвращает Comdat в модуле с указанным именем. Он создаётся, если его
     * ещё не существует.
     *
     * @see llvm::Module::getOrInsertComdat()
     */
    ЛЛКомдат ЛЛДайИлиВставьКомдат(ЛЛМодуль м, ткст0 имя);

    /**
     * Получить Comdat, присвоенный данному глобальному объекту.
     *
     * @see llvm::GlobalObject::getComdat()
     */
    ЛЛКомдат ЛЛДайКомдат(ЛЛЗначение зн);

    /**
     * Присвоить Comdat данному глобальному объекту.
     *
     * @see llvm::GlobalObject::setComdat()
     */
    проц ЛЛУстКомдат(ЛЛЗначение зн, ЛЛКомдат конст);

    /*
     * Получить тип выбора разрешения конфликта для Comdat.
     *
     * @see llvm::Comdat::getSelectionKind()
     */
    ЛЛВидВыбораКомдат ЛЛДайТипВыбораКомдат(ЛЛКомдат конст);

    /*
     * Установить тип выбора разрешения конфликта для Comdat.
     *
     * @see llvm::Comdat::setSelectionKind()
     */
    проц ЛЛУстТипВыбораКомдат(ЛЛКомдат конст, ЛЛВидВыбораКомдат вид);

}